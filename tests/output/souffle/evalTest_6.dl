.decl A(x_0:symbol, x_1:symbol)
.output A(delimiter=",")
.decl B(x_0:symbol, x_1:symbol)
.output B(delimiter=",")
.decl C(x_0:symbol, x_1:symbol, x_2:symbol)
.output C(delimiter=",")
A("A", "B").
B("D", "E").
C("C11", "C12", "C13").
A(x, y) :- B(y, x).
B(x, y) :- A(y, x).
B(x, x) :- C(x, f1, f2).
B(x, x) :- C(f1, x, f2).
B(x, x) :- C(f1, f2, x).
